<?xml version="1.0"?>


<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.com.pingan.sm.pojo">

    <class name="CaseInfo" table="case_info">
        <id name="id" column="ID">
            <generator class="assigned"/>
        </id>
        <property name="description" type="java.lang.String" column="description"/>
 	    <property name="relevance" type="java.lang.String" column="relevance"/>
 	    <property name="starttime" type="java.sql.Timestamp" column="starttime"/>
 	    <property name="endtime" type="java.sql.Timestamp" column="endtime"/>
 	    <property name="scope" type="java.lang.String" column="scope"/>
 	    <property name="solution" type="java.lang.String" column="solution"/>
 	    <property name="reason" type="java.lang.String" column="reason"/>
 	    <property name="remark" type="java.lang.String" column="remark"/>
 	    <property name="mins" type="java.lang.Float" column="mins"/>
 	    
    </class>

</hibernate-mapping>
