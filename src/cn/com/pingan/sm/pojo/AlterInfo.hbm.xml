<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.com.pingan.sm.pojo">

    <class name="AlterInfo" table="alter_info">
        <id name="id" type="java.lang.Integer" column="id" >
            <generator class="assigned"/>
        </id>
        <property name="alterdate" type="java.sql.Date" column="alter_date"/>
        <property name="content" type="java.lang.String" column="content"/>
        <property name="sysname" type="java.lang.String" column="sysname"/>
        <property name="risk" type="java.lang.String" column="risk"/>
        <property name="isagree" type="java.lang.String" column="is_agree"/>
        <property name="execute" type="java.lang.String" column="execute"/>
        <property name="rechecker" type="java.lang.String" column="rechecker"/>
        <property name="executer" type="java.lang.String" column="executer"/>
        <property name="recheck" type="java.lang.String" column="recheck"/>
        <property name="platform" type="java.lang.String" column="platform"/>
        
    </class>

</hibernate-mapping>
